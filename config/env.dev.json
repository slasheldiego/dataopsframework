{
  "env_name": "dev",
  "database": "demo",
  "input_csv_path": "/dbfs/FileStore/data/customers.csv",
  "bronze_table": "bronze_customers",
  "silver_table": "silver_customers",
  "gold_table": "gold_customers",
  "event_log_table": "ops_event_log",
  "dq_results_table": "ops_dq_results",
  "primary_key": "customer_id",
  "expectations": {
    "not_null": [
      "customer_id",
      "name",
      "email"
    ],
    "unique": [
      "customer_id"
    ],
    "range": {
      "age": [
        18,
        120
      ]
    },
    "regex": {
      "email": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]+$"
    }
  }
}